<%= form_for(@place) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br />
  <%= f.label :latitude %>
  <%= f.text_field :latitude %><br />
  <%= f.label :longitude %>
  <%= f.text_field :longitude %><br />
  <%= f.text_field :tag_list, placeholder: 'tag1, tag2, tag3' %>
  <%= f.submit %>
<% end %>
